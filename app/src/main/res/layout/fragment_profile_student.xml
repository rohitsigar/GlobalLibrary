                    <de.hdodenhof.circleimageview.CircleImageView
                        android:id="@+id/id_Profile_Image_student"
                        android:layout_width="100dp"
                        android:layout_height="100dp"
                        android:layout_alignParentStart="true"
                        android:layout_alignParentTop="true"
                        android:layout_alignParentEnd="true"
                        android:layout_alignParentBottom="true"
                        android:src="@drawable/ic_student__1_" />
                    <ImageView
                        android:layout_width="40dp"
                        android:layout_height="40dp"
                        android:id="@+id/change_image_button_student"
                        android:layout_alignParentEnd="true"
                        android:visibility="visible"
                        android:layout_centerVertical="true"
                        android:contentDescription="For more ui/ux visit www.materialuiux.com"
                        android:background="@drawable/ic_add"
                        app:tint="@color/dark_main_color" />
                    <ImageView
                        android:layout_width="40dp"
                        android:layout_height="40dp"
                        android:id="@+id/change_upload_student"
                        android:layout_alignParentEnd="true"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:contentDescription="For more ui/ux visit www.materialuiux.com"
                        android:visibility="gone"
                        android:background="@drawable/ic_check_done" />

                </RelativeLayout>

