                        android:background="@drawable/profile_background"
                        android:contentDescription="For more ui/ux visit www.materialuiux.com"
                        android:padding="3dp"
                        android:fontFamily="@font/quicksand_regular"
                        android:textStyle="bold"
                        android:id="@+id/change_student_dob_edit_student_profile"
                        android:layout_margin="5dp"
                        android:textColor="@color/color_primary_text"
                        android:textSize="14sp" />
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="1"
                    android:layout_marginStart="20dp"
                    android:layout_marginEnd="20dp"
                    android:contentDescription="For more ui/ux visit www.materialuiux.com"
                    android:orientation="vertical">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:contentDescription="For more ui/ux visit www.materialuiux.com"
                        android:text="Residential Address"
                        android:fontFamily="@font/quicksand_regular"
                        android:textStyle="bold"
                        android:layout_margin="5dp"
                        android:textColor="@color/dark_main_color"
                        android:textSize="12sp" />


                    <EditText
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/profile_background"
                        android:contentDescription="For more ui/ux visit www.materialuiux.com"
                        android:padding="3dp"
                        android:fontFamily="@font/quicksand_regular"
                        android:textStyle="bold"
                        android:layout_margin="5dp"
                        android:id="@+id/change_student_residential_address_edit_student_profile"
                        android:textColor="@color/color_primary_text"
                        android:textSize="14sp" />
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="1"
                    android:layout_marginStart="20dp"
                    android:layout_marginEnd="20dp"
                    android:contentDescription="For more ui/ux visit www.materialuiux.com"
                    android:orientation="vertical">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:contentDescription="For more ui/ux visit www.materialuiux.com"
                        android:text="Email Address"
                        android:fontFamily="@font/quicksand_regular"
                        android:textStyle="bold"
                        android:layout_margin="5dp"
                        android:textColor="@color/dark_main_color"
                        android:textSize="12sp" />


                    <EditText
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/profile_background"
                        android:contentDescription="For more ui/ux visit www.materialuiux.com"
                        android:layout_margin="5dp"
                        android:id="@+id/change_student_email_address_edit_student_profile"
                        android:padding="3dp"
                        android:fontFamily="@font/quicksand_regular"
                        android:textStyle="bold"
                        android:textColor="@color/color_primary_text"
                        android:textSize="14sp" />
                </LinearLayout>
